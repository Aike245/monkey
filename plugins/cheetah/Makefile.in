CC 	= $CC
CFLAGS	= $CFLAGS
LDFLAGS = $LDFLAGS
DEFS    = $DEFS
INCDIR  = ../../src/include

CHEETAH_OBJECTS = cheetah.o loop.o cmd.o cutils.o


all:

monkey-cheetah:	$(CHEETAH_OBJECTS)
	$(CC) $(CFLAGS) $(LDFLAGS) $(DEFS) -shared -Wl -o $@ $^ -lc

.c.o:
	$(CC) $(CFLAGS) $(LDFLAGS) $(DEFS) -I$(INCDIR) -fPIC -c $<
clean:
	rm -rf *.o *~
	rm -rf *.*so*
