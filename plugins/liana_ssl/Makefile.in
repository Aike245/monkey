CC 	= $CC
CFLAGS	= $CFLAGS
LDFLAGS = $LDFLAGS
DEFS    = $DEFS
INCDIR  = ../../src/include

all:
	$(CC) $(CFLAGS) $(LDFLAGS) $(DEFS) -I$(INCDIR) -I/usr/include/matrixssl -DLINUX -fPIC -c liana_ssl.c
	$(CC) $(CFLAGS) $(LDFLAGS) $(DEFS) -shared -o monkey-liana_ssl.so liana_ssl.o -lmatrixssl
clean:
	rm -rf *.o *~ *.*so*
