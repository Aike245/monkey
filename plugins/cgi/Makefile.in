all: monkey-cgi.so
include ../Make.common

CC	= @echo "  CC   $(_PATH)/$@"; $CC
CC_QUIET= @echo -n; $CC
AR      = @echo "  AR   $(_PATH)/$@"; ar
CFLAGS	= $CFLAGS
LDFLAGS = $LDFLAGS
DEFS    = $DEFS
OBJECTS = cgi.o request.o event.o

-include $(OBJECTS:.o=.d)

monkey-cgi.so: $(OBJECTS)
	$(CC) $(CFLAGS) $(LDFLAGS) $(DEFS) -shared -o $@ $^ -lc

monkey-cgi.a: $(OBJECTS)
	$(AR) rcs $@ $(OBJECTS)
