all: monkey-mbedtls.so
include ../Make.common

CC	= @echo "  CC   $(_PATH)/$@"; $CC
CC_QUIET= @echo -n; $CC
CFLAGS	= $CFLAGS
LDFLAGS = $LDFLAGS -lmbedtls
DEFS    = $DEFS
OBJECTS = mbedtls.o

-include $(OBJECTS:.o=.d)

monkey-mbedtls.so: $(OBJECTS)
	$(CC) $(CFLAGS) $(DEFS) -shared -o $@ $^ -lc $(LDFLAGS)
